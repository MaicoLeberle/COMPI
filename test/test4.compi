class Program {

	int x;

	void set_x(int z) {
		x = z;
	}

	int return_x() {
		return x;
	}

	int read_int () extern;

	void print_string(string s) extern;
	
	void main () {
		set_x(read_int());
		if (x == 1 || false) 
			print_string("x == 1 || false\n");
		else 
			if (x == 2)
				print_string("x == 2\n");
			else 
				print_string("x >= 3\n");
